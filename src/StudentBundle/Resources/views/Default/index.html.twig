<table>
    <tr>
        <td>Nr</td>
        <td>Imię</td>
        <td>Nazwisko</td>
        <td>Data urodzenia</td>
    </tr>
{% for student in students %}
    <tr>
        <td>{{ loop.index }}</td>
        <td>{{ student.name }}</td>
        <td>{{ student.surname }}</td>
        <td>{{ student.birthdate|date("d-m-Y") }}</td>
    </tr>
{% endfor %}
</table>
<p>Łącznie studentów: {{ studentsAmount }}</p>